<Project>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\build\SharpFuzz.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Nemanja Mijailovic</Authors>
    <RepositoryUrl>https://github.com/Metalnem/sharpfuzz.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/Metalnem/sharpfuzz/master/LICENSE</PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>Copyright Â© 2018, Nemanja Mijailovic</Copyright>
    <PackageTags>fuzzing testing fuzz-testing fuzzer</PackageTags>
  </PropertyGroup>
</Project>
